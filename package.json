{
  "name": "recipe-app",
  "version": "1.0.0",
  "scripts": {
    "devStart": "nodemon ./src/server.ts",
    "start": "ts-node ./src/server.ts",
    "lint": "eslint '**/*.ts'",
    "lint-fix": "eslint --fix **/*.ts",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/test/**/*.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.0.1",
    "cookie-parser": "1.4.5",
    "express": "4.17.1",
    "express-validator": "6.12.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "5.12.11",
    "passport": "0.4.1",
    "passport-http": "0.3.0",
    "passport-jwt": "4.0.0"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "watch": [
      "src"
    ],
    "ext": "ts, html, json",
    "ignore": [
      "src/public",
      ".git",
      "node_modules/"
    ]
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/chai": "4.2.21",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.12",
    "@types/faker": "5.5.7",
    "@types/jsonwebtoken": "8.5.3",
    "@types/mocha": "8.2.3",
    "@types/mongoose": "5.11.97",
    "@types/node": "16.0.0",
    "@types/passport": "1.0.7",
    "@types/passport-http": "0.3.9",
    "@types/passport-jwt": "3.0.6",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "dotenv": "10.0.0",
    "faker": "5.5.3",
    "mocha": "9.0.2",
    "nodemon": "2.0.9",
    "nyc": "15.1.0",
    "ts-mocha": "8.0.0",
    "ts-node": "10.0.0",
    "typescript": "4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Abrafaks/recipe.git"
  },
  "bugs": {
    "url": "https://github.com/Abrafaks/recipe/issues"
  },
  "homepage": "https://github.com/Abrafaks/recipe#readme"
}
